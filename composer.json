    "require": {
        "php": ">=5.4.0",
        "ext-pdo": "*",
        "doctrine/dbal": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
